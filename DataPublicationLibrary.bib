@article{akhtar_home_2006,
	title = {Home range and management of sloth bear (Melursus ursinus) in disturbed   and unprotected habitat of North Bilaspur Forest Division, Chhattisgarh,   India},
	volume = {132},
	lccn = {0000},
	abstract = {The North Bilaspur forest division harbour large number of sloth bears.   Due to habitat loss, fragmentation and encroachment of, forests,and   interspersion of crop fields, the bear population causing lot of   nuisance. Incidence of human killing and mauling by bears and   retaliatory killing of bears are quite frequent. Sloth bears were radio   collared in order to know movement patterns in such disturbed and   unprotected areas so that management of these bears can be done in   efficient and scientific manner. Six sloth bears were radio collared   during the study but only three bears could be monitored for longer   period as two of them lost collar and one died. As compare to Protected   Areas in Chitwan National Park, Nepal and Mudumalai Wildlife sanctuary   bears covered large home ranges. Male bears showed 22.33 km(2) and 20.01   km(2) home range at 95\% {MCP} whereas female had 7.8 km(2) which was   significantly less to the area covered by males. gears were found to   spent much of their time in human habitation and agriculture fields.   Habitats loss, fragmentation of forests, and less food availability were   the major reason behind movement of bears in human inhabited areas and   thus resulted in high rate of conflict. It is suggested that fruit   bearing species should be planted in the forest areas to restore the   sloth bear habitats along with providing protection to the large   contiguous patches of forests. The sloth bear population from isolated   den sites should be trauslocated to other suitable areas for reducing   human-bear conflict as well as for the conservation of sloth bears.},
	language = {English},
	number = {{12A}},
	journal = {Indian Forester},
	author = {Akhtar, Naim and Bargali, H. S. and Chauhan, N. P. S.},
	month = dec,
	year = {2006},
	pages = {123--131},
	file = {ISI Web of Knowledge Record:/Users/jkratz/Library/Application Support/Zotero/Profiles/khr3r5j0.default/zotero/storage/59EBDSVU/full_record.html:text/html}
},

