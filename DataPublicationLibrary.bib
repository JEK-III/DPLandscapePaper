@article{koler-matznick_new_2007,
	title = {The New Guinea singing dog: its status and scientific importance},
	volume = {29},
	shorttitle = {The New Guinea singing dog},
	url = {http://www.publish.csiro.au/paper/AM07005},
	abstract = {Time is running out for the opportunity to study the New Guinea singing dog (\textit{Canis hallstromi }Troughton 1957) to determine if they are a unique taxon. These wild canids are being threatened with hybridization from increasing numbers of imported domestic dogs (\textit{Canis familiaris}). The controversies over their taxonomy and its status in New Guinea exist because they live in areas infrequently visited even by local residents. Thus, evidence concerning its origins, behaviour, anatomy, and phylogenetics is inadequate. The morphological, physiological, and {DNA} data we currently have available are from the few remaining captive representatives held in zoos and private facilities. The singing dog may be an important evolutionary link to humankind's most ancient domesticated mammal, \textit{C. familiaris}, and to the Australian dingo (\textit{Canis dingo}). We refute the allegation that the singing dog originated as a feral modern \textit{C. familiaris}, or as hybrids of\textit{ C. dingo} with \textit{C. familiaris}, by recounting their known history within the cultural context of New Guinea. We point out some of their distinctive characteristics, including their unique estrus cycles, and outline their potential scientific merit. We conclude by offering an alternative taxonomic hypothesis.},
	number = {1},
	urldate = {2013-01-09},
	journal = {Australian Mammalogy},
	author = {Koler-Matznick, J and Yates, {B.C.} and Bulmer, S. and Brisbin, {I..L.} Jr.},
