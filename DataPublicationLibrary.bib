	journal = {Journal of Molecular Histology},
	author = {Manjeshwar, Sharmila and Lerner, Megan R and Zang, Xiao-Ping and Branam, Dannielle E and Pento, J Thomas and Lane, Mary M and Lightfoot, Stan A and Brackett, Daniel J and Jupe, Eldon R},
	month = aug,
