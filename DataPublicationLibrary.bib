@article{rajagopal_observations_1984,
	title = {Observations on the natural enemies of Odontotermes wallonensis   (Wasmann) (Isoptera: Termitidae) in south India.},
	volume = {4},
	lccn = {0004},
	shorttitle = {Observations on the natural enemies of Odontotermes wallonensis   (Wasmann) (Isoptera},
	abstract = {Termites were found more vulnerable to the attack of several natural   enemies particularly during swarming, construction and expansion of   mounds and while foraging outside. During the present investigation on   the mound building termite Odontotermes wallonensis (Wasmann), several   natural enemies were encountered which included invertebrates,   vertebrates and also a fungal parasite. The vertebrates were wind   scorpion, spiders, dragonfly, mantis, assasin bugs, ants, carabid   beetles. Among the vertebrates, frogs and toads, geckos, birds, bat,   scaly-anteater and sloth bear were observed. The birds were common on   the termites both during day and night time. The only fungal parasite   observed on the reproductives of this termite was Beauveria brongniartii   (Sacc.) The results of these observations are discussed in this paper.},
	language = {English},
