	year = {2009},
	note = {{PMID:} 19632223},
	keywords = {Adaptor Proteins, Vesicular Transport, Cell Membrane, Cholesterol, Clathrin-Coated Vesicles, Cytoplasm, Lipid Microdomain, Lipid Raft, Membrane Lipids, Membrane Microdomains, Membrane Proteins, review},
	pages = {2871--2878}
},

