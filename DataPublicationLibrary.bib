@article{thornton_early_2008,
	title = {Early body condition, time budgets and the acquisition of foraging skills in meerkats},
	volume = {75},
	issn = {0003-3472},
	lccn = {0019},
	url = {http://www.sciencedirect.com/science/article/pii/S0003347207004496},
	doi = {10.1016/j.anbehav.2007.08.004},
	abstract = {Early body condition commonly has important downstream effects on fitness. One, as yet unexplored, mechanism behind these effects may be that condition in early life affects time budgets and hence opportunities to learn critical skills. Meerkat, Suricata suricatta, pups must choose between begging for food from helpers and foraging for themselves. I found that pups in good condition early in life invested more time in foraging than individuals in poor condition and subsequently developed greater foraging efficiency, which was maintained in later life. Young pups spent an average of 18\% of their time foraging, even though all pups were initially incompetent foragers and gained few direct benefits from their attempts. Pups whose hunger was reduced through experimental provisioning increased their investment in foraging. This suggests that investment in foraging is mediated by available energy reserves and raises the possibility that, although foraging is energetically costly, pups may gain long-term benefits by practising. Surprisingly, manipulating body weight through long-term experimental provisioning did not result in increased investment in foraging or improved foraging efficiency. Possible explanations for this result are considered. The findings presented here provide some support for the hypothesis that high body condition allows individuals to invest time in costly foraging practice, leading to the development of skills. These effects, acting in tandem with other processes such as differential neural development, may help to explain common links between early condition and future fitness.},
	number = {3},
	urldate = {2012-10-11},
	journal = {Animal Behaviour},
	author = {Thornton, Alex},
	month = mar,
	year = {2008},
	keywords = {downstream effects, early condition, foraging skills, learning, meerkat, Suricata suricatta},
	pages = {951--962},
	file = {ScienceDirect Snapshot:/Users/jkratz/Library/Application Support/Zotero/Profiles/khr3r5j0.default/zotero/storage/ZNZGKZCH/S0003347207004496.html:text/html;Thornton_2008_Early body condition, time budgets and the acquisition of foraging skills in.pdf:/Users/jkratz/Library/Application Support/Zotero/Profiles/khr3r5j0.default/zotero/storage/APZ2EVUJ/Thornton_2008_Early body condition, time budgets and the acquisition of foraging skills in.pdf:application/pdf}
},

@article{tenenbaum_sea_2013,
	title = {A sea of standards for omics data: sink or swim?},
	issn = {, 1527-{974X}},
	shorttitle = {A sea of standards for omics data},
	url = {http://jamia.bmj.com/content/early/2013/09/27/amiajnl-2013-002066},
	doi = {10.1136/amiajnl-2013-002066},
	abstract = {In the era of big data, omic-scale technologies, and increasing calls for data sharing, it is generally agreed that the use of community-developed, open data standards is critical. Far less agreed upon is exactly which data standards should be used, the criteria by which one should choose a standard, or even what constitutes a data standard. It is impossible simply to choose a domain and have it naturally follow which data standards should be used in all cases. The ‘right’ standards to use is often dependent on the use case scenarios for a given project. Potential downstream applications for the data, however, may not always be apparent at the time the data are generated. Similarly, technology evolves, adding further complexity. Would-be standards adopters must strike a balance between planning for the future and minimizing the burden of compliance. Better tools and resources are required to help guide this balancing act.},
	language = {en},
	urldate = {2013-10-01},
	journal = {Journal of the American Medical Informatics Association},
	author = {Tenenbaum, Jessica D. and Sansone, Susanna-Assunta and Haendel, Melissa},
	month = sep,
	year = {2013},
