	urldate = {2010-04-16},
	journal = {Biochimica Et Biophysica Acta},
	author = {Da Cruz, Sandrine and Parone, Philippe A and Gonzalo, Philippe and Bienvenut, Willy V and Tondera, Daniel and Jourdain, Alexis and Quadroni, Manfredo and Martinou, Jean-Claude},
