	keywords = {Aging, Amino Acid Sequence, Animals, Base Sequence, Conserved Sequence, Embryonic and Fetal Development, Evolution, Genes, Tumor Suppressor, Humans, Molecular Sequence Data, Proteins, Repressor Proteins},
	pages = {99--124}
},

