	keywords = {Adolescent, Adult, Child, Child, Preschool, {DNA} Mutational Analysis, Female, Glucocorticoids, Heterozygote, Humans, Infant, Infant, Newborn, Intracellular Signaling Peptides and Proteins, Male, Membrane Proteins, Mutation, Nephrotic Syndrome, {NPHS2}, Podocin, Polymorphism, Genetic, Prednisone, Treatment Failure},
	pages = {722--732},
