	keywords = {Cell Aging, Cell Division, Cell Respiration, {DNA}, Ribosomal, Electron Transport, Longevity, Mitochondria, Mutation, phb-1, phb-2, prohibitin, Proteins, Repressor Proteins, Saccharomyces cerevisiae},
	pages = {149--157}
},

