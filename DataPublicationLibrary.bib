@article{ducker_broad_1962,
	title = {Broad behavior tendencies and otogeny of Surikatens (Suricata suricatta Schrub, Viverridae)},
	volume = {19},
	lccn = {0000},
	doi = {10.1163/156853962X00078},
	abstract = {The ontogeny of the behavior of two young Suricata suricatta was studied   from birth to 10 months of age. Sounds, movements, feeding, grooming,   threatening, defence and playing and the behavior of the mother are   described. The bodily development of the young was also studied. Voices,   sleeping positions, scraping, stretching, threatening, and defence   reactions are similar to those of other Viverridae and especially   species of the same subfamily of Herpestinae. For comparing the behavior   of Suricata with that of other Camivora, several components are found   corresponding to those of Mustelidae]. Although that family seems not to   be related taxonomically to Suricata some behavior components seem to be   homologous and not just analogous. During a short period of their   development only young Suricata cover the pit for their excrements and   later on they only scratch out such a pit but do not cover it, after   deposing their excrements. This suggests that the juvenile behavior   trait might be vestigial. {\textbar}{\textbar} {ABSTRACT} {AUTHORS:} Author},
