	note = {{PMID:} 15916950},
	keywords = {Animals, Base Sequence, Caenorhabditis elegans, Caenorhabditis elegans Proteins, Cilia, Computational Biology, Gene Expression Profiling, Genomics, Green Fluorescent Proteins, Mutation, Neurons, Protein Transport, Sequence Analysis, {DNA}, sto-2, Transcription Factors},
