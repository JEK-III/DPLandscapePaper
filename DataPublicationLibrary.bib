	keywords = {Anesthetics, Animals, Caenorhabditis elegans, Chromosome Mapping, Dose-Response Relationship, Drug, Drug Hypersensitivity, Enflurane, Ether, Ethyl, Halothane, Isoflurane, Models, Genetic, Mutation, unc-1, unc-9},
