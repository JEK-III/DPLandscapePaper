	keywords = {Amino Acid Sequence, Cardiolipins, Mitochondria, Molecular Sequence Data, phb-1, phb-2, Phosphatidylethanolamines, prohibitin, Repressor Proteins, Saccharomyces cerevisiae, Saccharomyces cerevisiae Proteins, Sequence Alignment},
