@article{demuth_polymer_2013,
	title = {Polymer multilayer tattooing for enhanced {DNA} vaccination},
	volume = {12},
	copyright = {© 2013 Nature Publishing Group},
	issn = {1476-1122},
	url = {http://www.nature.com/nmat/journal/v12/n4/full/nmat3550.html},
	doi = {10.1038/nmat3550},
	abstract = {{DNA} vaccines have many potential benefits but have failed to generate robust immune responses in humans. Recently, methods such as in vivo electroporation have demonstrated improved performance, but an optimal strategy for safe, reproducible, and pain-free {DNA} vaccination remains elusive. Here we report an approach for rapid implantation of vaccine-loaded polymer films carrying {DNA}, immune-stimulatory {RNA}, and biodegradable polycations into the immune-cell-rich epidermis, using microneedles coated with releasable polyelectrolyte multilayers. Films transferred into the skin following brief microneedle application promoted local transfection and controlled the persistence of {DNA} and adjuvants in the skin from days to weeks, with kinetics determined by the film composition. These ‘multilayer tattoo’ {DNA} vaccines induced immune responses against a model {HIV} antigen comparable to electroporation in mice, enhanced memory T-cell generation, and elicited 140-fold higher gene expression in non-human primate skin than intradermal {DNA} injection, indicating the potential of this strategy for enhancing {DNA} vaccination.},
	language = {en},
	number = {4},
	urldate = {2013-03-24},
	journal = {Nature Materials},
	author = {{DeMuth}, Peter C. and Min, Younjin and Huang, Bonnie and Kramer, Joshua A. and Miller, Andrew D. and Barouch, Dan H. and Hammond, Paula T. and Irvine, Darrell J.},
	month = apr,
	year = {2013},
	keywords = {Biological materials, Biomedical materials, Polymers},
	pages = {367--376},
	file = {Full Text PDF:/Users/jkratz/Library/Application Support/Zotero/Profiles/khr3r5j0.default/zotero/storage/CF9KVR3S/DeMuth et al. - 2013 - Polymer multilayer tattooing for enhanced DNA vacc.pdf:application/pdf;Snapshot:/Users/jkratz/Library/Application Support/Zotero/Profiles/khr3r5j0.default/zotero/storage/63GSNZHV/nmat3550.html:text/html}
