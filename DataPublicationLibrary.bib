	keywords = {Animals, Caenorhabditis, Interneurons, Lasers, Mechanoreceptors, Motor Neurons, Movement, Muscle Contraction, Nervous System Physiological Phenomena, Neural Pathways, {PVC}, Synapses, Touch},
	pages = {956--964}
},

