	keywords = {Anesthetics, Animals, Base Sequence, Blood Proteins, Caenorhabditis elegans, Caenorhabditis elegans Proteins, {DNA} Primers, Helminth Proteins, Membrane Proteins, Protein Binding, unc-1, unc-8},
	pages = {1673--1682},
