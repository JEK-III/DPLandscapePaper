	note = {{PMID:} 17709317},
	keywords = {Adult, Aged, Blood Proteins, Breast Neoplasms, Female, Humans, Immunohistochemistry, Membrane Proteins, Middle Aged, Neoplasm Metastasis, Prognosis, Receptor, {erbB-2}, stl-1, {STML-2}, Survival Analysis, Tumor Markers, Biological},
	pages = {430--436}
},

@article{sato_human_1993,
