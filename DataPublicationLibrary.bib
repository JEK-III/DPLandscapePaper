	keywords = {Actins, B-Lymphocytes, Blood Proteins, Cells, Cultured, human, Humans, Interleukin-2, Lymphocyte Activation, Lymphoid Tissue, Membrane Proteins, Protein Binding, Protein Transport, Receptors, Antigen, T-Cell, {RNA}, Small Interfering, slp-2, stl-1, T-Lymphocytes, Up-Regulation},
	pages = {1927--1936}
},

@article{ruf_patients_2004,
