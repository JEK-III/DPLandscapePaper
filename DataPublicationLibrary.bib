	keywords = {Cell Line, Fetus, Fluorescent Antibody Technique, Humans, Intercellular Junctions, Intracellular Signaling Peptides and Proteins, Kidney, Kidney Glomerulus, Membrane Proteins, Microscopy, Confocal, Microscopy, Immunoelectron, Podocin, {RNA}, Messenger, Tissue Distribution},
