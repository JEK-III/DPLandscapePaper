	keywords = {{DON'T} {SEND}},
	pages = {580--591},
	annote = {



Gauthier-Pilters H. 1967. The fennec. Afr Wildl 21:117–125. 



},
	file = {Valdespino_2007_Physiological Constraints and Latitudinal Breeding Season in the Canidae.pdf:/Users/jkratz/Library/Application Support/Zotero/Profiles/khr3r5j0.default/zotero/storage/3V75FNFW/Valdespino_2007_Physiological Constraints and Latitudinal Breeding Season in the Canidae.pdf:application/pdf}
