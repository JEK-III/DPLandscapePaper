@article{goldstein_cloning_2003,
	title = {Cloning and characterization of {SLP3:} a novel member of the stomatin family expressed by olfactory receptor neurons},
	volume = {4},
	issn = {1525-3961},
	shorttitle = {Cloning and characterization of {SLP3}},
