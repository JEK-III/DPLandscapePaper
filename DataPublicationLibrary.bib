	keywords = {Amiloride, Animals, Caenorhabditis elegans, Caenorhabditis elegans Proteins, Electric Conductivity, Mechanoreceptors, Mechanotransduction, Cellular, Membrane Proteins, Mutation, Physical Stimulation, Sodium, Sodium Channels, Touch},
	pages = {43--50}
},

