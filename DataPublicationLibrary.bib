	keywords = {Animals, Bacteria, Caenorhabditis elegans, Caenorhabditis elegans Proteins, Cell Aggregation, Cyclic {GMP}, Feeding Behavior, Food, Gases, Guanylate Cyclase, Heme, Hyperoxia, Ion Channels, Mutation, Neurons, Afferent, Nitric Oxide, Oxygen, {PQR}, Protein Binding, Protein Structure, Tertiary, Social Behavior, sto-5},
