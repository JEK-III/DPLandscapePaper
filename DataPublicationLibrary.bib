	urldate = {2010-05-08},
	journal = {Journal of Cellular Physiology},
	author = {Roskams, A J and Friedman, V and Wood, C M and Walker, L and Owens, G A and Stewart, D A and Altus, M S and Danner, D B and Liu, X T and {McClung}, J K},
