	note = {{PMID:} 12750157},
	keywords = {Anemia, Hemolytic, Blood Proteins, Blotting, Western, Bone Marrow Cells, Cell Size, Erythrocyte Membrane, Erythrocytes, Abnormal, Female, Flow Cytometry, Gene Expression, Humans, Male, Membrane Microdomains, Membrane Proteins, Pedigree, Stomatin, stomatocytosis},
	pages = {2268--2277}
},

